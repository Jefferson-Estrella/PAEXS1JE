<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PAEXS1JE.Reporte">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="lblusuario" FontSize="14" HorizontalTextAlignment="End" TextColor="Red"/>
            <Entry x:Name="txtNombre" IsReadOnly="True"/>
            <Entry x:Name="txtApellido" IsEnabled="False"/>
            <Entry x:Name="txtSalario" IsReadOnly="True"/>
            <Entry x:Name="txtDireccion" IsEnabled="False"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>